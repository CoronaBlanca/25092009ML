local library = loadfile("https://raw.githubusercontent.com/CoronaBlanca/25092009UI/refs/heads/main/Dr%20-%20Rxy%20By%20Shxryx")()

local window = library:AddWindow("Galaxy Hub", {
    main_color = Color3.fromRGB(75, 0, 130),
    min_size = Vector2.new(400, 500),
    can_resize = true,
})

local mainTab = window:AddTab("Main")
local farmFolder = mainTab:AddFolder("Auto Farm")

farmFolder:AddSwitch("Auto Punch", function(value)
    print("Auto Punch:", value)
end)

farmFolder:AddButton("Rebirth", function()
    print("Rebirth hecho!")
end)
